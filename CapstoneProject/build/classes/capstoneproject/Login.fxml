<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #5CDB95;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="capstoneproject.LoginController">
   <children>
      <GridPane layoutX="300.0" layoutY="255.0" prefHeight="80.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="UsernameLabel" alignment="CENTER" prefHeight="30.0" prefWidth="100.0" textFill="#05386b">
               <font>
                  <Font name="Arial Bold" size="15.0" />
               </font>
            </Label>
            <Label fx:id="Passwordlabel" alignment="CENTER" prefHeight="30.0" prefWidth="100.0" textFill="#05386b" GridPane.rowIndex="1">
               <font>
                  <Font name="Arial Bold" size="15.0" />
               </font>
            </Label>
            <TextField fx:id="Usernamefield" prefHeight="20.0" prefWidth="100.0" GridPane.columnIndex="1" />
            <PasswordField fx:id="Passwordfield" prefHeight="20.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Hyperlink fx:id="Signuplink" layoutX="285.0" layoutY="335.0" onAction="#changeScreenButtonPushed2">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
      </Hyperlink>
      <Button fx:id="Loginbut" layoutX="513.0" layoutY="273.0" mnemonicParsing="false" onAction="#changeScreenButtonPushed" textFill="#05386b">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font></Button>
      <Label fx:id="Frasesarea" alignment="CENTER" layoutX="100.0" layoutY="436.0" prefHeight="150.0" prefWidth="600.0" style="-fx-border-style: dashed; -fx-border-width: 2; -fx-border-color: #05386B;" textAlignment="JUSTIFY" textFill="#05386b" wrapText="true">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
         <padding>
            <Insets left="15.0" right="15.0" />
         </padding>
      </Label>
      <ImageView fx:id="title" fitHeight="100.0" fitWidth="450.0" layoutX="175.0" layoutY="14.0" pickOnBounds="true" />
      <Label fx:id="wrongpass" layoutX="512.0" layoutY="300.0" textFill="RED">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
